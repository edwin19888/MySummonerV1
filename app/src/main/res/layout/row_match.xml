<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/RelativeLayoutMatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <View
            android:id="@+id/ViewWinOrLose"
            android:layout_width="5dp"
            android:layout_height="90dp"
            android:background="#ff0101">
        </View>

        <RelativeLayout
            android:id="@+id/RelativeLayoutFondoWinOrLose"
            android:layout_width="fill_parent"
            android:layout_height="90dp"
            android:layout_toRightOf="@+id/ViewWinOrLose"
            android:background="#f1d0d0">

            <RelativeLayout
                android:id="@+id/rlDetalleUno"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <android.support.v7.widget.CardView
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/cvChampion"
                    app:cardCornerRadius="30dp"
                    android:innerRadius="0dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="5dp"
                    android:shape="ring"
                    android:thicknessRatio="1.9">

                    <ImageView
                        android:id="@+id/ivChampion"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true">
                    </ImageView>
                </android.support.v7.widget.CardView>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rlDetalleLevel"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginTop="40dp">

                <TextView
                    android:layout_height="20dp"
                    android:layout_width="20dp"
                    android:id="@+id/tvLevelPlayer"
                    android:background="@drawable/rounded_textview"
                    android:textColor="#ffffff"
                    android:gravity="center"
                    android:text="18">
                </TextView>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@+id/rlDetalleUno">

                <TextView
                    android:id="@+id/tvGameMode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tvGameCreation"
                    android:layout_alignBottom="@+id/tvGameCreation"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:text="ARAM"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/tvGameCreation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tvGameDurationn"
                    android:layout_alignBottom="@+id/tvGameDurationn"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="21dp"
                    android:layout_toLeftOf="@+id/tvGameDurationn"
                    android:layout_toStartOf="@+id/tvGameDurationn"
                    android:text="19/01/2018"
                    android:textSize="12dp" />


                <TextView
                    android:id="@+id/tvGameDurationn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="13dp"
                    android:layout_marginRight="13dp"
                    android:text="1h 36m 10s"
                    android:textSize="12dp" />



            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/rlDetalleLevel"
                android:layout_toEndOf="@+id/rlDetalleUno"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@+id/rlDetalleUno">

                <ImageView
                    android:id="@+id/ivSpell1"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:background="@drawable/empty" />

                <ImageView
                    android:id="@+id/ivSpell2"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:layout_below="@+id/ivSpell1"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/empty" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/relativeLayout"
                android:layout_toEndOf="@+id/relativeLayout"
                android:layout_toRightOf="@+id/relativeLayout">

                <android.support.v7.widget.CardView
                    android:id="@+id/cvItem00"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="1dp"
                    android:elevation="12dp"
                    android:innerRadius="0dp"
                    android:shape="ring"
                    android:thicknessRatio="1.9"
                    app:cardCornerRadius="5dp">

                    <ImageView
                        android:id="@+id/ivItem00"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/empty">
                    </ImageView>
                </android.support.v7.widget.CardView>


                <android.support.v7.widget.CardView
                    android:id="@+id/cvItem01"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="1dp"
                    android:layout_toRightOf="@+id/cvItem00"
                    android:elevation="12dp"
                    android:innerRadius="0dp"
                    android:shape="ring"
                    android:thicknessRatio="1.9"
                    app:cardCornerRadius="5dp">

                    <ImageView
                        android:id="@+id/ivItem01"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/empty">

                    </ImageView>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/cvItem02"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="1dp"
                    android:layout_toRightOf="@+id/cvItem01"
                    android:elevation="12dp"
                    android:innerRadius="0dp"
                    android:shape="ring"
                    android:thicknessRatio="1.9"
                    app:cardCornerRadius="5dp">

                    <ImageView
                        android:id="@+id/ivItem02"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/empty">

                    </ImageView>
                </android.support.v7.widget.CardView>


                <android.support.v7.widget.CardView
                    android:id="@+id/cvItem03"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="1dp"
                    android:layout_toRightOf="@+id/cvItem02"
                    android:elevation="12dp"
                    android:innerRadius="0dp"
                    android:shape="ring"
                    android:thicknessRatio="1.9"
                    app:cardCornerRadius="5dp">

                    <ImageView
                        android:id="@+id/ivItem03"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/empty">

                    </ImageView>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/cvItem04"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="1dp"
                    android:layout_toRightOf="@+id/cvItem03"
                    android:elevation="12dp"
                    android:innerRadius="0dp"
                    android:shape="ring"
                    android:thicknessRatio="1.9"
                    app:cardCornerRadius="5dp">

                    <ImageView
                        android:id="@+id/ivItem04"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/empty">

                    </ImageView>
                </android.support.v7.widget.CardView>


                <android.support.v7.widget.CardView
                    android:id="@+id/cvItem05"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="1dp"
                    android:layout_toRightOf="@+id/cvItem04"
                    android:elevation="12dp"
                    android:innerRadius="0dp"
                    android:shape="ring"
                    android:thicknessRatio="1.9"
                    app:cardCornerRadius="5dp">

                    <ImageView
                        android:id="@+id/ivItem05"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/empty">

                    </ImageView>
                </android.support.v7.widget.CardView>

                <TextView
                    android:id="@+id/tvWinOrDefeat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/cvItem05"
                    android:layout_toRightOf="@+id/cvItem05"
                    android:layout_marginTop="12dp"
                    android:layout_marginLeft="5dp"
                    android:text="D" />


            </RelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rlDetalleUno"
                    android:layout_marginLeft="5dp">

                    <TextView
                        android:id="@+id/tvKDA"
                        android:layout_width="55dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textSize="12dp"
                        android:text="6/11/13" />

                    <TextView
                        android:id="@+id/tvTypeKill"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/tvKDA"
                        android:layout_toRightOf="@+id/tvKDA"
                        android:gravity="center_vertical"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="12dp"
                        android:visibility="visible" />


                    <ImageView
                        android:id="@+id/ivCS"
                        android:layout_width="15dp"
                        android:layout_height="18dp"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignTop="@+id/ivGold"
                        android:layout_marginRight="10dp"
                        android:background="@drawable/minion" />

                    <TextView
                        android:id="@+id/tvCSPlayer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_toLeftOf="@+id/ivCS"
                        android:layout_toStartOf="@+id/ivCS"
                        android:text="322"
                        android:textSize="12dp" />

                    <ImageView
                        android:id="@+id/ivGold"
                        android:layout_width="10dp"
                        android:layout_height="15dp"
                        android:layout_alignParentBottom="true"
                        android:layout_toLeftOf="@+id/tvCSPlayer"
                        android:layout_toStartOf="@+id/tvCSPlayer"
                        android:background="@drawable/gold" />

                    <TextView
                        android:id="@+id/tvGold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_toLeftOf="@+id/ivGold"
                        android:layout_toStartOf="@+id/ivGold"
                        android:text="16.5K"
                        android:textSize="12dp" />


                </RelativeLayout>




        </RelativeLayout>

    </RelativeLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="3dp"
        android:background="#ffffff"
        android:layout_below="@+id/RelativeLayoutMatch"/>
 </RelativeLayout>